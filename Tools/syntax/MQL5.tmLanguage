	<key>repository</key>
	<dict>
		<key>comments</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>begin</key>
					<string>/\*</string>
					<key>end</key>
					<string>\*/</string>
					<key>name</key>
					<string>comment.block.mql5</string>
					<key>patterns</key>
					<array>
						<dict>
							<key>include</key>
							<string>#todo_markers</string>
						</dict>
					</array>
				</dict>
				<dict>
					<key>match</key>
					<string>//.*$</string>
					<key>name</key>
					<string>comment.line.double-slash.mql5</string>
					<key>patterns</key>
					<array>
						<dict>
							<key>include</key>
							<string>#todo_markers</string>
						</dict>
					</array>
				</dict>
			</array>
		</dict>
		<key>todo_markers</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>comment</key>
					<string>TODO/FIXME/XXX/BUG markers - flag for attention</string>
					<key>match</key>
					<string>\b(TODO|FIXME|XXX|BUG|HACK|UNDONE)\b:?.*</string>
					<key>name</key>
					<string>invalid.illegal.todo.mql5</string>
				</dict>
				<dict>
					<key>comment</key>
					<string>NOTE/OPTIMIZE/WARNING markers - informational flags</string>
					<key>match</key>
					<string>\b(NOTE|OPTIMIZE|WARNING|DEPRECATED|REFACTOR|REVIEW)\b:?.*</string>
					<key>name</key>
					<string>invalid.warning.note.mql5</string>
				</dict>
			</array>
		</dict>
		<key>strings</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>begin</key>
					<string>"</string>
					<key>end</key>
					<string>"</string>
					<key>name</key>
					<string>string.quoted.double.mql5</string>
					<key>patterns</key>
					<array>
						<dict>
							<key>match</key>
							<string>\\.</string>
							<key>name</key>
							<string>constant.character.escape.mql5</string>
						</dict>
					</array>
				</dict>
				<dict>
					<key>begin</key>
					<string>'</string>
					<key>end</key>
					<string>'</string>
					<key>name</key>
					<string>string.quoted.single.mql5</string>
					<key>patterns</key>
					<array>
						<dict>
							<key>match</key>
							<string>\\.</string>
							<key>name</key>
							<string>constant.character.escape.mql5</string>
						</dict>
					</array>
				</dict>
			</array>
		</dict>
		<key>preprocessor</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>match</key>
					<string>#\s*(include|import)\s*[&lt;"](.*?)[&gt;"]</string>
					<key>captures</key>
					<dict>
						<key>1</key>
						<dict>
							<key>name</key>
							<string>keyword.control.preprocessor.mql5</string>
						</dict>
						<key>2</key>
						<dict>
							<key>name</key>
							<string>string.quoted.other.mql5</string>
						</dict>
					</dict>
				</dict>
				<dict>
					<key>match</key>
					<string>#\s*(define|undef|ifdef|ifndef|else|endif|property|resource)\b</string>
					<key>name</key>
					<string>keyword.control.preprocessor.mql5</string>
				</dict>
			</array>
		</dict>
		<key>deprecated_mql4</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>comment</key>
					<string>DEPRECATED: MQL4 event handlers - replace with MQL5 OnInit/OnDeinit/OnTick</string>
					<key>match</key>
					<string>\b(init|deinit|start)\b(?=\s*\()</string>
					<key>name</key>
					<string>invalid.deprecated.mql4.event-handler.mql5</string>
				</dict>
				<dict>
					<key>comment</key>
					<string>DEPRECATED: MQL4 trade functions - use MQL5 CTrade class or OrderSend</string>
					<key>match</key>
					<string>\b(OrderClose|OrderCloseBy|OrderDelete|OrderModify|OrderClosePrice|OrderCloseTime|OrderComment|OrderCommission|OrderExpiration|OrderLots|OrderMagicNumber|OrderOpenPrice|OrderOpenTime|OrderPrint|OrderProfit|OrderSelect|OrderStopLoss|OrderSwap|OrderSymbol|OrderTakeProfit|OrderTicket|OrderType|OrdersHistoryTotal|OrdersTotal)\b</string>
					<key>name</key>
					<string>invalid.deprecated.mql4.trade-function.mql5</string>
				</dict>
				<dict>
					<key>comment</key>
					<string>DEPRECATED: MQL4 account functions</string>
					<key>match</key>
					<string>\b(AccountBalance|AccountCredit|AccountCompany|AccountCurrency|AccountEquity|AccountFreeMargin|AccountFreeMarginCheck|AccountFreeMarginMode|AccountLeverage|AccountMargin|AccountName|AccountNumber|AccountProfit|AccountServer|AccountStopoutLevel|AccountStopoutMode)\b</string>
					<key>name</key>
					<string>invalid.deprecated.mql4.account-function.mql5</string>
				</dict>
				<dict>
